@model List<ContractAvarageVM>
<!DOCTYPE html>
<html>
<head>
    <title>Contracts List</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Şirket</th>
                <th>Vade</th>
                <th>Pozisyon</th>
                <th>Adet</th>
                <th>Fiyat</th>
                <th>Ortalama</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var contract in Model)
            {
                <tr>
                    <td>@contract.Company</td>
                    <td>@contract.Expiry</td>
                    <td>@contract.SelectedPosition</td>
                    <td>@contract.Quantity</td>
                    <td>@contract.Price</td>
                    <td>@contract.ContactAverage</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
